			"name": "6-GetDataFromArray",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const resData = pm.response.json();\r",
							"const filterData = resData.json.filters;\r",
							"console.log(filterData);\r",
							"const countryData = filterData.find(e => e.name =='COUNTRY');\r",
							"console.log(countryData);\r",
							"pm.test(\"Check country data and array\", function () {\r",
							"    pm.expect(countryData).to.have.property('isAllowed', false);\r",
							"    pm.expect(countryData).to.be.exist;\r",
							"    pm.expect(filterData).to.be.an('array');\r",
							"    pm.expect(filterData.length).to.equal(3);\r",
							"    pm.expect(countryData).to.have.property('id', 102);\r",
							"    pm.expect(countryData).to.have.property('name', 'COUNTRY');\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
